{
  "manifest_version": 2,
  "name": "text2img",
  "description": "",
  "version": "0.1",
  "permissions": ["http://text2img.lucasou.com/"],
  "content_scripts": [
    {
      "matches":["http://www.facebook.com/*", "https://www.facebook.com/*"],
      "js": ["jquery-1.11.0.min.js", "main.js"],
      "css": ["main.css"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": ["images/*"]
}